{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/forbid-prop-types": [0],
    "react/require-default-props": [0],
    "react/prefer-stateless-function": [0],
    "react/destructuring-assignment": "never",
    "global-require": [0],
    "no-nested-ternary": [0],
    "no-mixed-operators": [0],
    "max-len": [0]
  }
}
